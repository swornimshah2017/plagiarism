/*responsive navigation*/
$(function() {
		alignMenu();

		$(window).resize(function() {
			$(".responsive-menu").append($(".responsive-menu li.hideshow ul").html());
			$(".responsive-menu li.hideshow").remove();
			alignMenu();
		});

		function alignMenu() {
			var w = 0;
			var mw = $(".responsive-menu").width() - 180;
			var i = -1;
			var menuhtml = '';
			jQuery.each($(".responsive-menu").children(), function() {
				i++;
				w += $(this).outerWidth(true);
				if (mw < w) {
					menuhtml += $('<div>').append($(this).clone()).html();
					$(this).remove();
				};
			});
			if(menuhtml){
				$(".responsive-menu").append(
						'<li  style="position:relative;" href="#" class="hideshow">'+
							'<a href="#">Others '+ 
							'<span class="caret"></span>'+
							'</a><ul>'+
						 menuhtml + '</ul></li>');
				$(".responsive-menu li.hideshow ul").css("top",
						$(".responsive-menu li.hideshow").outerHeight(true) + "px");
				$(".responsive-menu li.hideshow").click(function() {
					$(this).children("ul").toggle();
				});
			};
		};
	});
	

