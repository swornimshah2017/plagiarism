				$(c).removeClass('lazy');
				curImg.onload = function(){
					$(c).attr('src',this.src);
					$(c).removeAttr('data-original');
				}
				curImg.src = $(c).attr('data-original');
			}
		*/
		

        if (isElementVisible($(c),3000)) {
            src = $(c).attr('data-original');
			$(c).attr("src",src);
			$(c).removeAttr('data-original');
			$(c).removeClass('lazy');
			
		}



		
		/*
		isVisible = $(c).visible(true, false);
        if (isVisible) {
				var curImg = new Image();
				$(c).removeClass('lazy');
				curImg.onload = function(){
					$(c).attr('src',this.src);
					$(c).removeAttr('data-original');
				}
				curImg.src = $(c).attr('data-original');
		}
		*/
		
		/*
        isVisible = $(c).visible(true, false);
        if (isVisible) {
            src = $(c).attr('data-original');
			$(c).attr("src",src);
			$(c).load(function(){
				$(c).removeAttr('data-original');
				$(c).removeClass('lazy');
			})
		}	
		*/
		
            /*
			this must be used in other site then ekantipur.
			equalheight();*/
        
    });
}
